<% provide(:title, "search") %>
<div class="container">
	<h1>Log in</h1>

	<div class="row">
	  <div class="col-md-6 col-md-offset-3">
	    <%= form_for(:match, url: defaultmatch_path) do |f| %>

	      <%= f.submit "search", class: "btn btn-primary" %>
	    <% end %>

	    <p>New user? <%= link_to "home", index_path %></p>
	  </div>
	</div>
</div>